/*///////////////////////////////////////////////////////////////////////*/
/*************************************************************************/
/****************************     TEXTS     ******************************/
/*************************************************************************/
/*************************************************************************/

/* Text (XS) */
.text-xs {
  font-family: 'SF Pro';
  font-style: normal;
  font-weight: 400;
  font-size: 10px;
  line-height: 125%;
  color: #7C7C7C;
}

/* Text (SM) */
.text-sm {
  font-family: 'SF Pro';
  font-style: normal;
  font-weight: 400;
  font-size: 14px;
  line-height: 125%;
  color: #404040;
}

/* Subtitle (LG) */
.subtitle {
  font-family: 'SF Pro';
  font-style: normal;
  font-weight: 700;
  font-size: 20px;
  line-height: 125%;
  color: #4B4B4B;
}

/* Title (XL) */
.title {
  font-family: 'SF Pro';
  font-style: normal;
  font-weight: 700;
  font-size: 30px;
  line-height: 125%;
  color: #242424;
}

/*************************************************************************/
/*///////////////////////////////////////////////////////////////////////*/





/*///////////////////////////////////////////////////////////////////////*/
/*************************************************************************/
/****************************     COLORS     *****************************/
/*************************************************************************/
/*************************************************************************/

/* Primary Color */
.primary-color {
  background: #00326F;
}

/* Secondary Color */
.secondary-color {
  background: #F1F1F1;
}

/*************************************************************************/
/*///////////////////////////////////////////////////////////////////////*/





/*///////////////////////////////////////////////////////////////////////*/
/*************************************************************************/
/***************************     BUTTONS     *****************************/
/*************************************************************************/
/*************************************************************************/

/* Primary Button */
.primary-button {
  position: relative;
  width: auto;
  height: 45px;
  left: 28px;
  top: 198px;

  color: #F1F1F1;
  font-family: 'SF Pro';
  font-style: normal;
}

/* Secondary Button */
.secondary-button {
  position: relative;
  width: auto;
  height: 45px;
  left: 28px;
  top: 198px;

  color: #00326F;
  font-family: 'SF Pro';
  font-style: normal;
}

